{

  "RabbitMq": {
    "Host": "${RABBITMQ_HOST}",
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}"
  },
  "MongoDbSettings": {
    "ConnectionString": "${MONGO_CONNECTION_STRING}",
    "DatabaseName": "${MONGO_DATABASE_NAME}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
